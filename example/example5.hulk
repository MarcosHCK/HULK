print (factorial (9));

let a = 2 in {

  print (
    while (a < 10)
      {
        a := a + 1;
      }
  );
};

function factorial (n) => if (n < 2) 1 else n * factorial (n - 1);

protocol Changable
{
  set (a: number): number;
}

type Simple
{
  var: number = 9;

  set (n) => self.var := n;
  get () => self.var;
  dump () => print (self.var);
}

let o = new Simple () in {

  o.dump ();
  o.set (44);
  o.dump ();
};


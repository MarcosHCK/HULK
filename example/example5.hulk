function factorial (n) => if (n < 2) 1 else n * factorial (n - 1);

let a = 4 in
{
  while (a < 8)
    {

      print (a);
      a := a + 1;
    };

  print (factorial (a));
  print (a := 9);
};

type Simple
{
  var: number = 0;
  change (n) => self.var := n;
  dump () => print (self.var);
}
